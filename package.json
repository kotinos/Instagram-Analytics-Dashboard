{
  "name": "instagram-analytics-dashboard",
  "version": "0.1.0",
  "private": true,
  "description": "Desktop app for monitoring Instagram creators, scraping metrics, and visualizing analytics.",
  "author": "",
  "license": "MIT",
  "main": "public/electron/main.cjs",
  "scripts": {
    "dev": "concurrently \"cross-env VITE_DEV_SERVER=true vite --port 5173\" \"wait-on http://localhost:5173 && cross-env VITE_DEV_SERVER=true electron .\"",
    "build:renderer": "vite build",
    "build": "electron-builder --dir",
    "package": "electron-builder",
    "lint": "echo 'todo: add eslint'",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "rebuild:native": "electron-rebuild -f -w better-sqlite3",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@mui/x-data-grid": "^6.18.0",
    "@mui/x-date-pickers": "^6.18.0",
    "axios": "^1.6.0",
    "better-sqlite3": "^9.0.0",
    "chart.js": "^4.4.0",
    "date-fns": "^2.30.0",
    "electron-store": "^8.1.0",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.0",
    "puppeteer": "^21.0.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.10.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^27.0.0",
  "electron-builder": "^24.6.0",
  "electron-rebuild": "^3.2.9",
    "electron-devtools-installer": "^3.2.0",
    "typescript": "^5.3.2",
    "vite": "^4.5.0",
    "vitest": "^1.5.0",
    "wait-on": "^7.2.0"
  }
}
